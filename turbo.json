{
	"$schema": "https://turbo.build/schema.json",
	"ui": "tui",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": [".dist/**", "build/**"]
		},
		"lint": {
			"dependsOn": ["^lint"],
			"outputs": [".eslintcache"]
		},
		"check:types": {
			"dependsOn": ["^check:types"]
		},
		"check:unused": {
			"dependsOn": ["^check:unused"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		}
	}
}
