{
	"name": "react-hono-turbo",
	"private": true,
	"author": "Baatchia Riyadh",
	"devDependencies": {
		"@trivago/prettier-plugin-sort-imports": "^5.2.2",
		"cspell": "^9.0.1",
		"prettier": "^3.5.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"turbo": "^2.5.3"
	},
	"engines": {
		"node": ">=20"
	},
	"packageManager": "bun@1.1.38",
	"scripts": {
		"build": "turbo build",
		"dev": "turbo dev",
		"lint": "turbo lint",
		"format": "prettier . -w -u -c --cache",
		"check:spelling": "cspell . --cache",
		"check:types": "turbo check:types"
	},
	"workspaces": [
		"apps/*",
		"packages/*"
	]
}
